{
  "author": "Paul Miller (https://paulmillr.com)",
  "bugs": {
    "url": "https://github.com/paulmillr/chokidar/issues"
  },
  "dependencies": {
    "readdirp": "^4.0.1"
  },
  "description": "Minimal and efficient cross-platform file watching library",
  "devDependencies": {
    "@paulmillr/jsbt": "0.2.1",
    "@types/node": "20.14.8",
    "chai": "4.3.4",
    "prettier": "3.1.1",
    "rimraf": "5.0.5",
    "sinon": "12.0.1",
    "sinon-chai": "3.7.0",
    "typescript": "5.5.2",
    "upath": "2.0.1"
  },
  "engines": {
    "node": ">= 14.16.0"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./index.js"
    },
    "./handler.js": {
      "import": "./esm/handler.js",
      "require": "./handler.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "handler.js",
    "handler.d.ts",
    "esm"
  ],
  "funding": "https://paulmillr.com/funding/",
  "homepage": "https://github.com/paulmillr/chokidar",
  "keywords": [
    "fs",
    "watch",
    "watchFile",
    "watcher",
    "watching",
    "file",
    "fsevents"
  ],
  "license": "MIT",
  "main": "./index.js",
  "module": "./esm/index.js",
  "name": "chokidar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulmillr/chokidar.git"
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "format": "prettier --write src",
    "lint": "prettier --check src",
    "test": "node --test"
  },
  "sideEffects": false,
  "types": "./index.d.ts",
  "version": "4.0.3"
}
